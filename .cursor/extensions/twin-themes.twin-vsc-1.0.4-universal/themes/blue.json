  //////////////////////////////////////////////
  //              SYNTAX COLORS               //
  //////////////////////////////////////////////

  "tokenColors": [
    {
      "name": "Plain text",
      "scope": ["text", "text.html.markdown", "meta.object-literal.key"],
      "settings": {
        "foreground": "#BCC0CC"
      }
    },
    {
      "name": "Dots, semicolons, commas, hashtags, etc",
      "scope": [
        "punctuation",
        "punctuation.accessor",
        "punctuation.definition.entity",
        "comment.punctuation.comma.sass"
      ],
      "settings": {
        "foreground": "#14B369"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "text.html.basic entity.other.attribute-name"],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#DBEC79"
      }
    },
    {
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#BCC0CC"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#71BAEF60"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E47267"
      }
    },
    {
      "scope": ["entity.name", "entity.name.tag.css"],
      "settings": {
        "foreground": "#F4BF5D"
      }
    },
    {
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#F4BF5D"
      }
    },
    {
      "scope": ["keyword.control", "storage.type"],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["variable", "meta.block variable.other", "variable.other.readwrite"],
      "settings": {
        "foreground": "#BABDBC"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["variable.other.readwrite.class", "variable.other.readwrite.instance"],
      "settings": {
        "foreground": "#E47267"
      }
    },
    {
      "scope": ["keyword.control.conditional", "keyword.control.rust", "keyword.control.trycatch"],
      "settings": {
        "foreground": "#E47267"
      }
    },
    {
      "scope": ["entity.other.inherited-class", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#F4BF5D"
      }
    },
    {
      "name": "Braces, parentheses, etc",
      "scope": [
        "meta.brace",
        "punctuation.definition",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.string",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section",
        "punctuation.section.embedded",
        "punctuation.separator.inheritance.php",
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "constant.character.numeric",
        "support.function",
        "keyword.other.special-method",
        "string.other.link",
        "entity.name.command.shell",
        "storage.type.function.arrow.js",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#E47267"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F4BF5D"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "foreground": "#BCC0CC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#BCC0CC",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.python"],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "name": "PHP arrows",
      "scope": ["keyword.operator.class.php"],
      "settings": {
        "foreground": "#E47267"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "entity.name.type.class.php",
        "variable.function",
        "keyword.control.filter"
      ],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "name": "CSS, SASS, SCSS, Stylus property values",
      "scope": [
        "constant.language.color.rgb-value.css",
        "source.css",
        "punctuation.definition.constant",
        "constant.numeric",
        "keyword.other.unit",
        "keyword.control.unit.css",
        "support.constant.color.w3c-extended-color-name",
        "support.constant.font-name",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#C181E4"
      }
    },
    {
      "scope": ["source.ini", "source.fish", "source.shell"],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "name": "JSON property name",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markdown.heading",
        "punctuation.definition.heading.markdown",
        "entity.name.section.markdown",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "name": "Markdown quotes",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#71BAEF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "foreground": "#BCC0CC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown strikethrough",
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown underline",
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#DBEC79",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["source.go"],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#F4BF5D"
      }
    },
    {
      "scope": ["keyword", "variable.other.object.js"],
      "settings": {
        "foreground": "#25D483"
      }
    },
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#C181E4"
      }
    },
    {
      "scope": [
        "support.type.property-name.toml",
        "support.type.property-name.table.toml",
        "entity.name.tag.yaml",
        "meta.tag.sgml",
        "constant.other.option",
        "string.unquoted.argument.shell",
        "variable.other.property.php",
        "entity.other.inherited-class.php",
        "support.class.builtin",
        "support.class.php"
      ],
      "settings": {
        "foreground": "#71BAEF"
      }
    },
    {
      "scope": [
        "string.quoted.single.literal.line.toml",
        "string.unquoted.plain.out.yaml",
        "keyword.other",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#25D483"
      }
    }

